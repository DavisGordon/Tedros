<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <display-name>tedros</display-name>
 
  <servlet>
    <servlet-name>customerServletEn</servlet-name>
    <servlet-class>org.tedros.web.servlet.CustomerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>customerServletEn</servlet-name>
	<url-pattern>/en/suc</url-pattern>
  </servlet-mapping> 
  <servlet>
    <servlet-name>customerServletPt</servlet-name>
    <servlet-class>org.tedros.web.servlet.CustomerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>customerServletPt</servlet-name>
	<url-pattern>/pt/suc</url-pattern>
  </servlet-mapping> 

</web-app>
